per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
money = float(input('Сумма под проценты: '))
deposit = list (map (int, [money * per_cent.get('ТКБ')/100,
           money * per_cent.get('СКБ')/100,
           money * per_cent.get('ВТБ')/100,
           money*per_cent.get('СБЕР')/100]))
print(deposit)
print('Максимальная сумма, которую вы можете заработать -', max(deposit))
